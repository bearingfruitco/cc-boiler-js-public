{
  "version": "1.0",
  "$schema": "IMPORTANT: All API keys below are PLACEHOLDERS. Replace with your actual keys before use.",
  "mcpServers": {
    "brave-search": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-brave-search"],
      "env": {
        "BRAVE_API_KEY": "YOUR_BRAVE_API_KEY"
      },
      "disabled": false
    },
    "puppeteer": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-puppeteer"],
      "env": {
        "PUPPETEER_ALLOWED_URLS": "http://localhost:*"
      },
      "disabled": false
    },
    "stagehand": {
      "command": "npx",
      "args": ["-y", "@browserbasehq/stagehand-mcp"],
      "env": {
        "STAGEHAND_API_KEY": "YOUR_STAGEHAND_API_KEY"
      },
      "disabled": true
    },
    "browserbase": {
      "command": "npx",
      "args": ["-y", "@browserbasehq/browserbase-mcp"],
      "env": {
        "BROWSERBASE_API_KEY": "YOUR_BROWSERBASE_API_KEY",
        "BROWSERBASE_SESSION_TIMEOUT": "1800"
      },
      "disabled": true
    },
    "github": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-github"],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "YOUR_GITHUB_PAT"
      },
      "disabled": false
    },
    "supabase": {
      "command": "npx",
      "args": ["-y", "@supabase/mcp-server"],
      "env": {
        "SUPABASE_API_KEY": "YOUR_SUPABASE_SERVICE_ROLE_KEY"
      },
      "disabled": false
    },
    "Prisma-Local": {
      "command": "npx",
      "args": ["-y", "prisma", "mcp"]
    },
    "Prisma-Remote": {
      "command": "npx",
      "args": ["-y", "mcp-remote", "https://mcp.prisma.io/mcp"]
    },
    "Sentry": {
      "command": "npx",
      "args": ["-y", "mcp-remote@latest", "https://mcp.sentry.dev/mcp"]
    },
    "dbt-mcp": {
      "command": "uvx",
      "args": [
        "--env-file",
        ".env",
        "dbt-mcp"
      ]
    },
    "upstash": {
      "command": "npx",
      "args": ["-y", "@upstash/mcp-server"],
      "env": {
        "UPSTASH_REDIS_REST_URL": "YOUR_UPSTASH_URL",
        "UPSTASH_REDIS_REST_TOKEN": "YOUR_UPSTASH_TOKEN"
      },
      "disabled": true
    },
    "cloudflare-observability": {
      "command": "npx",
      "args": ["-y", "@cloudflare/mcp-server-observability"],
      "env": {
        "CLOUDFLARE_API_TOKEN": "YOUR_CLOUDFLARE_TOKEN",
        "CLOUDFLARE_ACCOUNT_ID": "YOUR_ACCOUNT_ID"
      },
      "disabled": true
    },
    "cloudflare-bindings": {
      "command": "npx",
      "args": ["-y", "@cloudflare/mcp-server-bindings"],
      "env": {
        "CLOUDFLARE_API_TOKEN": "YOUR_CLOUDFLARE_TOKEN",
        "CLOUDFLARE_ACCOUNT_ID": "YOUR_ACCOUNT_ID"
      },
      "disabled": true
    },
    "bright-data": {
      "command": "npx",
      "args": ["-y", "@brightdata/mcp-scraper"],
      "env": {
        "BRIGHT_DATA_CUSTOMER_ID": "YOUR_CUSTOMER_ID",
        "BRIGHT_DATA_API_KEY": "YOUR_API_KEY"
      },
      "disabled": true
    },
    "context7": {
      "command": "npx",
      "args": ["-y", "@context7/mcp"],
      "disabled": false
    }
  }
}
