    "tdd-engineer": {
      "required": [],
      "optional": ["github-mcp", "playwright-mcp"],
      "permissions": {
        "github-mcp": ["repos:manage"],
        "playwright-mcp": ["tests:execute"]
      }
    },
    
    "event-schema": {
      "required": [],
      "optional": ["bigquery-toolbox", "dbt-mcp", "sentry-mcp"],
      "permissions": {
        "bigquery-toolbox": ["tables:crud"],
        "dbt-mcp": ["transformations:run"],
        "sentry-mcp": ["errors:track"]
      }
    },
    
    "performance": {
      "required": [],
      "optional": ["sentry-mcp", "redis-mcp"],
      "permissions": {
        "sentry-mcp": ["performance:monitor"],
        "redis-mcp": ["cache:manage"]
      }
    },
    
    "refactoring-expert": {
      "required": [],
