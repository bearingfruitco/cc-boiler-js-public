  "agent_mcp_mappings": {
    "senior-engineer": {
      "required": ["github-mcp", "supabase-mcp", "sentry-mcp"],
      "optional": ["ref-tools-mcp", "octocode-mcp"],
      "permissions": {
        "github-mcp": ["repos:manage", "issues:crud", "prs:create"],
        "supabase-mcp": ["database:crud", "auth:management"],
        "sentry-mcp": ["errors:track", "issues:analyze"]
      }
    },
    
    "database-architect": {
      "required": ["supabase-mcp", "dbt-mcp", "bigquery-toolbox"],
      "optional": ["airbyte-mcp"],
      "permissions": {
        "supabase-mcp": ["database:crud", "rls:policies", "migrations:execute"],
        "dbt-mcp": ["models:create", "transformations:run"],
        "bigquery-toolbox": ["datasets:manage", "tables:crud"]
      }
    },
    
    "frontend": {
      "required": ["stagehand-mcp"],
      "optional": ["browserbase-mcp", "playwright-mcp", "better-auth-mcp"],
      "permissions": {
        "stagehand-mcp": ["browser:automate", "elements:interact"],
        "playwright-mcp": ["tests:execute", "screenshots:capture"]
      }
    },
    
    "backend": {
      "required": ["supabase-mcp"],
      "optional": ["redis-mcp", "upstash-mcp", "better-auth-mcp", "bigquery-toolbox"],
      "permissions": {
        "supabase-mcp": ["database:crud", "auth:management", "realtime:subscriptions"],
        "redis-mcp": ["cache:manage", "pubsub:channels"]
      }
    },
    
    "qa": {
      "required": ["playwright-mcp", "stagehand-mcp"],
      "optional": ["browserbase-mcp", "sentry-mcp"],
      "permissions": {
        "playwright-mcp": ["tests:execute", "browser:control", "network:intercept"],
        "stagehand-mcp": ["browser:automate", "forms:fill"],
        "sentry-mcp": ["errors:track", "issues:analyze"]
      }
    },
    
    "analytics-engineer": {
      "required": ["bigquery-toolbox", "dbt-mcp", "airbyte-mcp"],
      "optional": ["supabase-mcp"],
      "permissions": {
        "bigquery-toolbox": ["queries:execute", "analytics:run"],
        "dbt-mcp": ["models:create", "transformations:run"],
        "airbyte-mcp": ["pipelines:create", "sync:data"]
      }
    },
    
    "integration-specialist": {
      "required": ["airbyte-mcp"],
      "optional": ["supabase-mcp", "better-auth-mcp", "google-mcp-toolbox", "bright-data-mcp"],
      "permissions": {
        "airbyte-mcp": ["pipelines:create", "connectors:manage", "sync:data"],
        "google-mcp-toolbox": ["google:services", "sheets:manage"]
      }
    },
    
    "supabase-specialist": {
      "required": ["supabase-mcp"],
      "optional": ["better-auth-mcp", "dbt-mcp"],
      "permissions": {
        "supabase-mcp": ["*"],
        "better-auth-mcp": ["auth:flows", "oauth:integrate"]
      }
    },
    
    "playwright-specialist": {
      "required": ["playwright-mcp"],
      "optional": ["stagehand-mcp", "browserbase-mcp", "bright-data-mcp"],
      "permissions": {
        "playwright-mcp": ["*"],
        "stagehand-mcp": ["browser:automate"],
        "browserbase-mcp": ["cloud:browsers", "parallel:execution"]
      }
    },
    
    "security-auditor": {
      "required": ["github-mcp"],
      "optional": ["sentry-mcp", "better-auth-mcp", "supabase-mcp"],
      "permissions": {
        "github-mcp": ["repos:manage", "actions:trigger"],
        "sentry-mcp": ["errors:track", "alerts:manage"],
        "supabase-mcp": ["rls:policies"]
      }
    },
    
    "pii-guardian": {
      "required": [],
      "optional": ["supabase-mcp", "sentry-mcp", "redis-mcp"],
      "permissions": {
        "supabase-mcp": ["database:crud", "rls:policies"],
        "sentry-mcp": ["issues:analyze"],
        "redis-mcp": ["cache:manage"]
      }
    },
    
    "automation-workflow-engineer": {
      "required": ["github-mcp"],
      "optional": ["stagehand-mcp", "browserbase-mcp", "bright-data-mcp"],
      "permissions": {
        "github-mcp": ["actions:trigger", "repos:manage"],
        "stagehand-mcp": ["browser:automate"],
        "bright-data-mcp": ["scraping:web", "data:extract"]
      }
    },
    
    "researcher": {
      "required": ["exa-mcp"],
      "optional": ["bright-data-mcp", "serena-mcp"],
      "permissions": {
        "exa-mcp": ["search:semantic", "research:deep"],
        "bright-data-mcp": ["scraping:web"],
        "serena-mcp": ["ai:assistance"]
      }
    },
    
    "documentation-writer": {
      "required": [],
      "optional": ["github-mcp", "ref-tools-mcp"],
      "permissions": {
        "github-mcp": ["repos:manage"],
        "ref-tools-mcp": ["references:manage", "citations:format"]
      }
    },
    
    "migration-specialist": {
      "required": ["supabase-mcp"],
      "optional": ["dbt-mcp", "airbyte-mcp"],
      "permissions": {
        "supabase-mcp": ["migrations:execute", "database:crud"],
        "dbt-mcp": ["transformations:run"],
        "airbyte-mcp": ["sync:data"]
      }
    },
    
    "orm-specialist": {
      "required": ["supabase-mcp"],
      "optional": ["dbt-mcp"],
      "permissions": {
        "supabase-mcp": ["database:crud"],
        "dbt-mcp": ["models:create"]
