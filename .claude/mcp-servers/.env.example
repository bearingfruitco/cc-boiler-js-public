# MCP Server Environment Variables
# Copy this to .env and fill in your values

# Supabase
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_KEY=your-service-key-here

# GitHub
GITHUB_TOKEN=ghp_your_github_personal_access_token
GITHUB_OWNER=bearingfruitco
GITHUB_REPO=debt-funnel

# BigQuery (optional for now)
GCP_PROJECT=your-gcp-project
GOOGLE_APPLICATION_CREDENTIALS=./bigquery-key.json

# DBT (optional for now)
DBT_PROJECT_DIR=./dbt
DBT_PROFILES_DIR=./dbt
DBT_TARGET=prod

# Playwright (optional for now)
PLAYWRIGHT_HEADLESS=true
PLAYWRIGHT_TIMEOUT=30000

# Sentry (optional)
SENTRY_DSN=https://your-sentry-dsn
SENTRY_ORG=your-org
SENTRY_PROJECT=your-project

# Redis (optional)
REDIS_URL=redis://localhost:6379
UPSTASH_URL=https://your-upstash-url
UPSTASH_TOKEN=your-upstash-token
